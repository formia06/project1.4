<div class="container-fluid">
  <div class="table-responsive">
    <h2>Project Financials</h2>
    <table class="table table-striped table-sm">
      <tr>
        <th>Project Name</th>
        <th>Task Number</th>
        <th>Task</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Department</th>
        <th>Income</th>
        <th>Expenses</th>

      </tr>
      <%
                          if(projectFinance.length > 0)
                          {
                              projectFinance.forEach(function(projectFinance){
                          %>
      <tr>

        <td><a href="/projects/view/<%= projectFinance.projectName%>"> <%= projectFinance.projectName %></a></td>
        <td><%= projectFinance.taskNumber %></td>
        <td><%= projectFinance.task %></td>
        <td><%= projectFinance.startdate %></td>
        <td><%= projectFinance.enddate %></td>
        <td><%= projectFinance.department %></td>
        <td><%= projectFinance.incomeAmount %></td>
        <td><%= projectFinance.expensesAmount %></td>
      </tr>
      <%
                              });
                          }
                          else
                          {
                          %>
      <tr>
        <td colspan="5">No Data Found</td>
      </tr>
      <%
                          }
                          %>


    </table>
  </div>
</div>
